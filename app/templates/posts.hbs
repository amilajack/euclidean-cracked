{{#each model as |post|}}
  {{#edit-post
    post=post
    session=session
    onSavePost=(route-action 'save')
    delete='delete'
    createComment='createComment'
  }}
    {{add-track
      post=post
      session=session
      onCreateTrack=(route-action 'createTrack')
    }}
  {{/edit-post}}
{{/each}}

{{outlet}}
