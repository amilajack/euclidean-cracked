{{#if currentProject}}
  {{audio-project-sequencer project=currentProject readOnly=true}}
{{/if}}


{{#each projects as |project|}}
  {{#index-project-item project=project}}
    {{mdc-card-action-icon "play_arrow" title="Play" click=(action "setCurrentProject" project)}}
  {{/index-project-item}}
{{else}}
  There are no projects...
{{/each}}
